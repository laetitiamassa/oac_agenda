<%= simple_form_for(@orgn) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :oname %>
    <%= f.input :odesc %>
    <%= f.input :oemail %>
    <%= f.input :onum %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
